    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "drupal/core": "^10 || ^9 || ^8",
        "centretek/environment": "^1.0"
    },
    "authors": [
        {
            "name": "Patrick Wilson",
            "email": "pwilson@centretek.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Centretek\\Settings\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
